<%= render './users/sidebar' %>
<%= render './ancients/g' %>

<div class="all">
  <div class="total">
    <div>
      articles:
    </div>
    <div class="count">
      <%= external_count %>
    </div>
  </div>
  <div class="articles">
    <% external_article.each do |article| %>
    <div class="article-container">
       <div class="who-liked">
        <p><i class="fa fa-heart"></i>
          <%= who_liked(article) %>
        </p>
      </div>
      <div class="article-row">
        <div>
          <%= external_user(article) %>
        </div>
        <div class="article-second">
          <div class="article-body-height">
            <%= link_to article.body , article_path(article), :class => 'article-body'%>
          </div>
         <div class="hash-tag">
            #<%= article_hash_tag(article) %>
          </div>
        </div>
      </div>
    </div>
    <% end %><br>
  </div>
</div>